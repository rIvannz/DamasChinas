<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>

	<connectionStrings configSource="connectionStrings.config" />

	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding name="netTcp_NoSecurity" receiveTimeout="10675199.02:48:05.4775807">
					<security mode="None" />
				</binding>
			</netTcpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior>
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>

		<services>
			<service name="Damas_Chinas_Server.LoginService">
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="netTcp_NoSecurity"
						  contract="Damas_Chinas_Server.ILoginService" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8000/LoginService/" />
						<add baseAddress="net.tcp://localhost:8733/LoginService/" />
					</baseAddresses>
				</host>
			</service>

			<service name="Damas_Chinas_Server.SingInService">
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="netTcp_NoSecurity"
						  contract="Damas_Chinas_Server.ISingInService" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8001/SignInService/" />
						<add baseAddress="net.tcp://localhost:8734/SignInService/" />
					</baseAddresses>
				</host>
			</service>

			<service name="Damas_Chinas_Server.AccountManager">
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="netTcp_NoSecurity"
						  contract="Damas_Chinas_Server.IAccountManager" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8002/AccountManager/" />
						<add baseAddress="net.tcp://localhost:8739/AccountManager/" />
					</baseAddresses>
				</host>
			</service>

			<service name="Damas_Chinas_Server.ChatService">
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="netTcp_NoSecurity"
						  contract="Damas_Chinas_Server.IChatService" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8003/ChatService/" />
						<add baseAddress="net.tcp://localhost:8755/ChatService/" />
					</baseAddresses>
				</host>
			</service>

			<service name="Damas_Chinas_Server.FriendService">
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="netTcp_NoSecurity"
						  contract="Damas_Chinas_Server.IFriendService" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8004/FriendService/" />
						<add baseAddress="net.tcp://localhost:8740/FriendService/" />
					</baseAddresses>
				</host>
			</service>
		</services>
	</system.serviceModel>
</configuration>
